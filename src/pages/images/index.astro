---
import { Image } from "astro:assets";
import MainLayout from "src/layouts/MainLayout.astro";

import post05Img from "@images/post-05.png";
---

<MainLayout title="Imágenes">
  <h1 class="text-5xl text-white">Imágenes</h1>

  <img
    loading="eager"
    src="/assets/images/post-05.png"
    alt="imagen 5"
    width={250}
    height={250}
  />

  <Image src={post05Img} alt="imagen 5" width={250} height={250} />

  <div
    style={{
      height: "3000px",
    }}
  >
  </div>

  <img
    loading="lazy"
    src="/assets/images/post-01.png"
    alt="imagen 1"
    class="size-96"
  />
</MainLayout>
